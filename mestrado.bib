@ARTICLE{buteau2000csm,
  author = {Chantal Buteau and Mazzola, G.},
  title = {From Contour Similarity to Motivic Topologies},
  journal = {Musicae Scientiae},
  year = {2000},
  volume = {4},
  pages = {125--149},
  number = {2},
  abstract = {This paper is devoted to a qualitative and quantitative study of topological
	spaces built on premotif collections of musical scores. These motivic
	topologies are related to similarity concepts in the American music
	set theory. Through shapes, imitations and gestalts, and similarity
	relations between any two shapes, we obtain a motivic hierarchy f
	a score. This model of motivic analysis is completed by proposing
	different tools, such as weighted shapes and motivic evolution trees
	(MET), for visualizing these non-intuitive topologies. The concept
